{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!assets/|.*\\..*).*)",
            "destination": "/shem-pro-frontend/dist/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/shem-pro-frontend/dist/$1"
        }
    ]
}